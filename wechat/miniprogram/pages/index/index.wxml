<view class="wrapper page">
<!-- 登录页 wrapper main -->
  <view class="main">
    <view class="log">
      <image src="../../images/mark-github.svg"></image>
    </view>
    <tabs>
      <tab title="账号密码">
        <form bindsubmit="commitAccount" report-submit='{{true}}' class="=form">
          <view>
            <input name="username" placeholder="輸入賬號" focus/>
            <input name="password" placeholder="輸入密碼" password="{{true}}" value='{{password}}'/>            
          </view>
          <button class="login" formType='submit'>登陸</button>
        </form>
      </tab>
      <tab title="Token"></tab>
    </tabs>
  </view>
</view>